<html lang="en">

<head>

	{{> /partials/head }}

</head>

<body>

	<div class="wrapper">

		{{> /partials/sidebar }}

		<div class="main">

			{{> /partials/navbar }}

			<main class="content">

				<div class="container-fluid p-0">


					<h1 class="h3 mb-3">{{$ pageheader }}{{/ pageheader }}</h1>

					{{$ alerts }}
