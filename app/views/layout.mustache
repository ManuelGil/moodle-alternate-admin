					<div class="row">

						<div class="col-12">

							<div class="card">

								{{$ content }}
								{{/ content }}

							</div>

						</div>

					</div>

				</div>

			</main>

			{{> /partials/footer }}

		</div>

	</div>

	{{> /partials/scripts }}

</body>

</html>
